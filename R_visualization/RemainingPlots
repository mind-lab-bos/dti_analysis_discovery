		○ Remove 0's (Find and Replace), replace with empty cell
		○ Delete empty cells (ctrl+g), delete to left to align
		○ Copy and transpose so that first row is Part ID
	
	CC_body_Mean_Data <- FA_along_new_ROIs[c(1:46)]
	CC_body_Mean_Data <- CC_body_Mean_Data[-c(1)]
	CC_body_Mean_Data <- CC_body_Mean_Data[-c(2)]
	CC_body_Mean_Data_long <- melt(CC_body_Mean_Data, id.vars="PartID")
	Musicianship <-  FA_along_new_ROIs[c(2:3)]
	CC_body_Mean_Data_final <- merge(CC_body_Mean_Data_long, Musicianship, by.x="PartID")
	
	ggplot(data=CC_body_Mean_Data_final, aes(x=variable, y=value, group=PartID, colour=as.factor(pre_play_an_instrument)))+ geom_line() + geom_point() + ggtitle("CC Body Mean FA Along Tract") + theme(legend.position="none") + scale_color_manual(name="pre_play_an_instrument", values=c("grey", "dark red"), labels=c("nonmusician", "musician")) + theme(plot.title = element_text(hjust = 0.5)) + xlab("Point Along Tract") + ylab("CC Body FA") + theme(axis.text.x=element_blank(), axis.ticks.x=element_blank())
	
